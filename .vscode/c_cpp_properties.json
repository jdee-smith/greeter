{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/lib",
                "/usr/local"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/usr/lib",
                    "/usr/local"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },

            "defines": [],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks",
                "/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}